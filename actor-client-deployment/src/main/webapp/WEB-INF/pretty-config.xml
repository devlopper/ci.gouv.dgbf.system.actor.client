<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="identification">
		<pattern value="/identification" />
		<view-id value="/public/identification/index.jsf" />
	</url-mapping>
	
	<url-mapping id="creation_compte">
		<pattern value="/creation_compte.jsf" />
		<view-id value="/public/actor/create.jsf" />
	</url-mapping>
	
	<url-mapping id="demandes">
		<pattern value="/private/demandes.jsf" />
		<view-id value="/private/request/index.jsf" />
	</url-mapping>
	
	<url-mapping id="demande_compte">
		<pattern value="/demande_compte" />
		<view-id value="/public/accountrequest/edit.jsf" />
	</url-mapping>
	
	<url-mapping id="notification_demande_compte">
		<pattern value="/notification_demande_compte" />
		<view-id value="/public/accountrequest/notifyaftercreate.jsf" />
	</url-mapping>
	
	<url-mapping id="liste_demandes">
		<pattern value="/private/liste_demandes" />
		<view-id value="/private/accountrequest/list.jsf" />
	</url-mapping>
	
	<url-mapping id="liste_demandes_rejetees">
		<pattern value="/private/liste_demandes_rejetees" />
		<view-id value="/private/rejectedaccountrequest/list.jsf" />
	</url-mapping>
	
	<url-mapping id="liste_comptes">
		<pattern value="/private/liste_comptes" />
		<view-id value="/private/account/list.jsf" />
	</url-mapping>
	
	<url-mapping id="assignation">
		<pattern value="/private/assignation.jsf" />
		<view-id value="/private/actor/assignation.jsf" />
	</url-mapping>
	
	<url-mapping id="affectation">
		<pattern value="/private/affectation.jsf" />
		<view-id value="/private/scopefunction/affectation.jsf" />
	</url-mapping>
	
	<url-mapping id="domaine_administration">
		<pattern value="/private/domaineadministration.jsf" />
		<view-id value="/private/cluster/administrationdomains.jsf" />
	</url-mapping>
	
	<!-- Request -->
	
	<url-mapping id="demande_fonctions_budgetaires">
		<pattern value="/public/demande_fonctions_budgetaires.jsf" />
		<view-id value="/public/request/edit/index.jsf" />
	</url-mapping>
	
	<!-- Request dispatch slip -->
	
	<url-mapping id="bordereaux">
		<pattern value="/private/bordereaux.jsf" />
		<view-id value="/private/requestdispatchslip/index.jsf" />
	</url-mapping>
	
	<!-- Profile CEDAS -->
	
	<url-mapping id="das_affectations">
		<pattern value="/das/affectations.jsf" />
		<view-id value="/profile/das/affectations.jsf" />
	</url-mapping>
	
	<url-mapping id="das_demandes">
		<pattern value="/das/demandes.jsf" />
		<view-id value="/profile/das/demandes.jsf" />
	</url-mapping>
	
	<!--  -->
	
	<url-mapping id="mon_compte">
		<pattern value="/mon_compte" />
		<view-id value="/myaccount/index.jsf" />
	</url-mapping>
	
	<url-mapping id="mes_demandes">
		<pattern value="/mon_compte/demandes.jsf" />
		<view-id value="/myaccount/request/index.jsf" />
	</url-mapping>
	
	<url-mapping id="initier_demande">
		<pattern value="/mon_compte/demande/initier.jsf" />
		<view-id value="/myaccount/request/initialize.jsf" />
	</url-mapping>
	
	<url-mapping id="consulter_demande">
		<pattern value="/mon_compte/demande/consulter.jsf" />
		<view-id value="/myaccount/request/read.jsf" />
	</url-mapping>
	
	<url-mapping id="mes_visibilites">
		<pattern value="/mon_compte/visibilites" />
		<view-id value="/myaccount/listscopes.jsf" />
	</url-mapping>
	
	<url-mapping id="modifier_mot_passe">
		<pattern value="/mon_compte/modifier_mot_passe" />
		<view-id value="/myaccount/editpass.jsf" />
	</url-mapping>
	
	<url-mapping id="mes_preferences">
		<pattern value="/mon_compte/preferences" />
		<view-id value="/myaccount/preferences.jsf" />
	</url-mapping>
	
	<url-mapping id="mon_profile">
		<pattern value="/mon_compte/profile.jsf" />
		<view-id value="/myaccount/profile/read.jsf" />
	</url-mapping>
	
	<url-mapping id="modifier_mon_profile">
		<pattern value="/mon_compte/profile/modifier.jsf" />
		<view-id value="/myaccount/profile/edit.jsf" />
	</url-mapping>
	
</pretty-config>