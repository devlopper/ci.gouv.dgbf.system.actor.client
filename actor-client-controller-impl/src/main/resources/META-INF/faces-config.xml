<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>actorListView</from-outcome>
			<to-view-id>/private/actor/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorCreateView</from-outcome>
			<to-view-id>/private/actor/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorListPrivilegesView</from-outcome>
			<to-view-id>/private/actor/listprivileges.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorEditPrivilegesView</from-outcome>
			<to-view-id>/private/actor/editprivileges.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorListScopesView</from-outcome>
			<to-view-id>/private/actor/listscopes.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorEditScopesView</from-outcome>
			<to-view-id>/private/actor/editscopes.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorCreateScopesView</from-outcome>
			<to-view-id>/private/actor/createscopes.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actorCreatePrivilegesView</from-outcome>
			<to-view-id>/private/actor/createprivileges.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>scopeListView</from-outcome>
			<to-view-id>/private/scope/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>scopeTypeListView</from-outcome>
			<to-view-id>/private/scopetype/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>privilegeListView</from-outcome>
			<to-view-id>/private/privilege/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>privilegeTypeListView</from-outcome>
			<to-view-id>/private/privilegetype/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>profileListView</from-outcome>
			<to-view-id>/private/profile/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>profileTypeListView</from-outcome>
			<to-view-id>/private/profiletype/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>functionListView</from-outcome>
			<to-view-id>/private/function/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>functionTypeListView</from-outcome>
			<to-view-id>/private/functiontype/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>